// Prackage email provides html and text templates for emails
package email

templ WelcomeHtml(name string, link string) {
<html>

<body>
	<h1 style="background:#3874d3;color:#fff;padding:12px;font-size:14px">Hello, { name }</h1>
	<p>Get started: <a href={ link }>{ link }</a></p>
</body>

</html>
}
